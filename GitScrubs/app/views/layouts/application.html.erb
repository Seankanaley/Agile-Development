    <%= yield %>



    </div>


   <!-- Footer -->
  <footer class="py-5 footer">


    <div class="logo-name text-center">

      <%= image_tag("gitScrubsName_white.png") %>

    </div>
    <div class="container">
      <p class="m-0 text-center make_me_white">Copyright &copy; gitScrubs 2018</p>
    </div>
    <!-- /.container -->
  </footer>
  </body>
</html>
